<%- include('../layout', { body: `
  <h1>Leads</h1>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Nome</th>
        <th>Email</th>
        <th>Interesse</th>
        <th>Status</th>
      </tr>
    </thead>

    <tbody>
      ${leads.map(l => `
        <tr>
          <td>${l.name}</td>
          <td>${l.email}</td>
          <td>${l.interest}</td>
          <td>${l.status}</td>
        </tr>
      `).join('')}
    </tbody>
  </table>
` }) %>
